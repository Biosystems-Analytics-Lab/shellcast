{% extends "base.html.jinja" %}

{% block head %}
  {{ super() }}
  <!-- Custom JS -->
  <script src="{{ url_for('static', filename='index.js') }}"></script>
{% endblock %}

{% block content %}
  <div id="closure-map"></div>
  <script src="https://maps.googleapis.com/maps/api/js?key= {{- mapsAPIKey -}} &callback=initMap" async defer></script>

  <br>

  <h3>Closure Probabilities</h3>
  <table id="closure-table" class="table">
    <thead>
      <tr>
        <th scope="col">Growing Area</th>
        <th scope="col">3-day (%)</th>
        <th scope="col">2-day (%)</th>
        <th scope="col">1-day (%)</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
{% endblock %}
